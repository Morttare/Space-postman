;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Space postman"
run/main_scene="res://Scenes/Mainmenu.tscn"
config/features=PackedStringArray("4.3", "Forward Plus")
config/icon="res://icon.svg"

[autoload]

Dialogic="*res://addons/dialogic/Core/DialogicGameHandler.gd"
Global="*res://Scripts/Global.gd"

[dialogic]

directories/dch_directory={
"Administrator": "res://Dialogic/Administrator.dch",
"Grandma": "res://Dialogic/Grandma/Grandma.dch",
"Lost_Person": "res://Dialogic/Lost_Person.dch",
"Officer": "res://Dialogic/Receptionist/Officer.dch",
"Postman": "res://Dialogic/Grandma/Postman.dch",
"Postman_in_kela": "res://Dialogic/Postman_in_kela.dch",
"Receptionist": "res://Dialogic/Receptionist/Receptionist.dch"
}
directories/dtl_directory={
"Queue/person_with_queue_number_screen": "res://Dialogic/Queue/person_with_queue_number_screen.dtl",
"correct_queue": "res://Dialogic/Queue/correct_queue.dtl",
"final_dialogue": "res://Dialogic/final_dialogue.dtl",
"grandma_generic": "res://Dialogic/Grandma/grandma_generic.dtl",
"grandma_letters_found": "res://Dialogic/Grandma/grandma_letters_found.dtl",
"grandma_meet": "res://Dialogic/Grandma/grandma_meet.dtl",
"grandma_puzzle_in_progress": "res://Dialogic/Grandma/grandma_puzzle_in_progress.dtl",
"lost_person_again": "res://Dialogic/lost_person_again.dtl",
"lost_person_meet": "res://Dialogic/lost_person_meet.dtl",
"receptionist_again": "res://Dialogic/Receptionist/receptionist_again.dtl",
"receptionist_meet": "res://Dialogic/Receptionist/receptionist_meet.dtl",
"wrong_queue": "res://Dialogic/Queue/wrong_queue.dtl"
}
extensions_folder="res://addons/dialogic_additions"
text/letter_speed=0.01
text/initial_text_reveal_skippable=true
text/text_reveal_skip_delay=0.1
text/advance_delay=0.1
text/autoadvance_per_character_delay=0.1
text/autoadvance_ignored_characters_enabled=true
audio/max_channels=4.0
animations/join_default_length=0.5
animations/join_default_wait=true
animations/leave_default_length=0.5
animations/leave_default_wait=true
animations/cross_fade_default_length=0.5
choices/autofocus_first=true
choices/delay=0.2
choices/reveal_delay=0
choices/reveal_by_input=false
save/autosave_delay=60.0
save/encryption_on_exports_only=true
text/autopauses={}
variables={
"has_letter": false
}
layout/style_list=["res://Default_Style.tres", "res://Kela_Style.tres"]
layout/default_style="res://Default_Style.tres"
glossary/default_case_sensitive=true

[display]

window/size/viewport_width=1280
window/size/viewport_height=720
window/stretch/mode="viewport"

[editor_plugins]

enabled=PackedStringArray("res://addons/dialogic/plugin.cfg")

